@model CourseManagement.Models.Course

<h2>Détails Filière</h2>

<div>
    <h4>Information Filière</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Titre :</dt>
        <dd class="col-sm-10">@Model.Title</dd>
        <dt class="col-sm-2">Description :</dt>
        <dd class="col-sm-10">@Model.Description</dd>
        <dt class="col-sm-2">Status Filière :</dt>
        <dd class="col-sm-10">@Model.StatusC</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Modifier</a>
    <a asp-action="Index" class="btn btn-secondary">Retour a la liste</a>
</div>
